{
  "name": "react-ui-components",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run server & npm run watch:scripts & npm run watch:styles",
    "server": "browser-sync start --server --files='./**/index.html,./**/bundle.css,./**/bundle.js' --no-open --no-ui --ss examples",
    "build:scripts": "browserify examples/index.js -o examples/bundle.js -t babelify",
    "watch:scripts": "watchify examples/index.js -o examples/bundle.js -t babelify --debug",
    "build:styles": "postcss -c .postcssrc.js",
    "watch:styles": "postcss -c .postcssrc.js -w"
  },
  "dependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.6",
    "browserify": "^13.3.0",
    "classnames": "^2.2.5",
    "cssnano": "^3.10.0",
    "postcss-cli": "^2.6.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "watchify": "^3.8.0"
  }
}
